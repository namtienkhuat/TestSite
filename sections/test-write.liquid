{% schema %}
{
        "name": "FAQs",
        "tag": "section",
        "class": "section",
        "settings": [
                {
                        "type": "text",
                        "id": "caption",
                        "label": "Caption",
                },
                {
                        "type": "inline_richtext",
                        "id": "heading",
                        "label": "Heading",
                        "default": "Test write content config"
                },
                {
      "type": "select",
      "id": "heading_size",
      "options": [
        {
          "value": "h2",
          "label": "Small"
        },
        {
          "value": "h1",
          "label": "Medium"
        },
        {
          "value": "h0",
          "label": "Large"
        },
        {
          "value": "hxl",
          "label": "Extra large"
        },
        {
          "value": "hxxl",
          "label": "Extra extra large"
        }
      ],
      "default": "h1",
      "label": "Medium"
    },
     {
      "type": "checkbox",
      "id": "open_first_question",
      "default": false,
      "label": "Open first question"
    }, {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 36
    }

                ],
   "presets": [
    {
      "name": "FAQs Test Write",

    }
  ]
}
{% endschema %}
